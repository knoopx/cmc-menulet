{
  "name": "cmc-menulet",
  "productName": "CoinMarketCap",
  "author": "Víctor Martínez",
  "version": "0.2.1",
  "description": "CoinMarketCap Menulet",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "yarn build:icon && yarn build:clean && yarn build:webpack",
    "build:clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack",
    "build:icon": "mkdir -p Icon.iconset && sips -z 16 16 Icon.png --out Icon.iconset/icon_16x16.png && sips -z 32 32 Icon.png --out Icon.iconset/icon_16x16@2x.png && sips -z 32 32 Icon.png --out Icon.iconset/icon_32x32.png && sips -z 64 64 Icon.png --out Icon.iconset/icon_32x32@2x.png && sips -z 128 128 Icon.png --out Icon.iconset/icon_128x128.png && sips -z 256 256 Icon.png --out Icon.iconset/icon_128x128@2x.png && sips -z 256 256 Icon.png --out Icon.iconset/icon_256x256.png && sips -z 512 512 Icon.png --out Icon.iconset/icon_256x256@2x.png && sips -z 512 512 Icon.png --out Icon.iconset/icon_512x512.png && iconutil -c icns Icon.iconset && rm -rf Icon.iconset",
    "package": "electron-builder --publish always",
    "release": "cross-env NODE_ENV=production yarn run build && yarn run package",
    "release:all": "cross-env NODE_ENV=production yarn run build && yarn run package:all",
    "production": "cross-env NODE_ENV=production yarn run build && yarn run start",
    "development": "cross-env NODE_ENV=development yarn run start"
  },
  "build": {
    "appId": "net.knoopx.CoinMarketCap",
    "files": [
      "main.js",
      "IconTemplate*",
      "dist/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "mac": {
      "icon": "Icon.icns",
      "target": "dir",
      "category": "public.app-category.finance",
      "publish": [
        "github"
      ]
    }
  },
  "dependencies": {
    "electron-default-menu": "^1.0.1",
    "menubar": "^5.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-classes": "^7.1.0",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@vx/group": "^0.0.170",
    "@vx/responsive": "^0.0.179",
    "@vx/scale": "^0.0.179",
    "@vx/shape": "^0.0.179",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-autobind-class-methods": "^5.0.1",
    "babel-plugin-closure-elimination": "^1.3.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.19",
    "babel-preset-react-optimize": "^1.0.1",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "cryptocoins-icons": "^2.8.0",
    "css-loader": "^1.0.0",
    "electron": "^3.0.5",
    "electron-builder": "^20.28.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.11",
    "mobx": "^5.5.2",
    "mobx-react": "^5.3.5",
    "mobx-state-tree": "^3.7.0",
    "mobx-utils": "^5.0.4",
    "numeral": "^2.0.6",
    "open-color": "^1.6.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.8.8",
    "react-icons": "^3.2.1",
    "react-motion": "^0.5.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "tachyons": "^4.11.1",
    "tachyons-open-color": "knoopx/tachyons-open-color",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
