{
  "name": "cmc-menulet",
  "description": "CoinMarketCap Menulet",
  "author": "Víctor Martínez",
  "version": "1.0.1",
  "main": "main.js",
  "homepage": "https://knoopx.github.io/cmc-menulet/",
  "scripts": {
    "electron:dev": "electron-webpack-dev-server",
    "electron:compile:main": "webpack-cli --mode production --config webpack.main.config.js",
    "electron:compile:renderer": "webpack-cli --mode production --config webpack.renderer.config.js",
    "electron:compile": "yarn electron:compile:main && yarn electron:compile:renderer",
    "electron:release": "yarn electron:compile && electron-builder",
    "electron:release:all": "yarn electron:compile && electron-builder -mwl",
    "web:dev": "webpack-dev-server --hot --mode development --config webpack.web.config.js",
    "web:compile": "webpack-cli --mode production --config webpack.web.config.js",
    "web:publish": "yarn web:compile && gh-pages -d dist/web"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": "release/*.*"
        }
      ]
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-classes": "^7.7.4",
    "@babel/plugin-transform-react-constant-elements": "^7.7.4",
    "@babel/plugin-transform-react-inline-elements": "^7.7.4",
    "@babel/preset-env": "^7.7.5",
    "@babel/preset-react": "^7.7.4",
    "@fullhuman/purgecss-loader": "^1.0.0",
    "@hot-loader/react-dom": "^16.11.0",
    "@vx/grid": "^0.0.192",
    "@vx/group": "^0.0.192",
    "@vx/responsive": "^0.0.192",
    "@vx/scale": "^0.0.192",
    "@vx/shape": "^0.0.192",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "classnames": "^2.2.6",
    "core-js": "^3.4.7",
    "cryptocoins-icons": "^2.9.0",
    "css-loader": "^3.2.1",
    "d3-time": "^1.1.0",
    "electron": "^7.1.3",
    "electron-builder": "^21.2.0",
    "electron-webpack-dev-server": "knoopx/electron-webpack-dev-server",
    "eslint-config-knoopx": "knoopx/eslint-config-knoopx",
    "extract-css-chunks-webpack-plugin": "^4.7.1",
    "file-loader": "^5.0.2",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.15",
    "menubar": "^7.1.0",
    "mobx": "^5.15.0",
    "mobx-react": "^6.1.4",
    "mobx-state-tree": "^3.15.0",
    "mobx-utils": "^5.5.2",
    "open-color": "^1.7.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "purgecss-whitelister": "^2.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-icons": "^3.8.0",
    "react-router-dom": "^5.1.2",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.1",
    "tailwindcss": "^1.1.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "webpack-pwa-manifest": "^4.1.1"
  }
}
